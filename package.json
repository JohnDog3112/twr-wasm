{
  "name": "tiny-wasm-runtime",
  "version": "0.9.3",
  "description": "An easy to use wasm runtime to execute C code in a browser using Web Assembly.",
  "keywords": [
    "wasm",
    "wasm runtime",
    "Web Assembly",
    "Web Assembly Runtime",
    "runtime",
    "crt",
    "light",
    "lightweight",
    "easy",
    "simple",
    "small"
  ],
  "files": [
    "./examples/*/*.html",
    "./examples/*/*.c",
    "./examples/*/*.wasm",
    "./examples/*.py",
    "./examples/*/package.json",
    "./examples/*/Makefile*",
    "./include/*",
    "./lib-c/*",
    "./lib-js/*",
    "./LICENSE",
    "./readme.md"
  ],
  "module": "./lib-js/index.js",
  "exports": {
    ".": "./lib-js/index.js",
    "./twrWasmModule": "./lib-js/twrmod.js",
    "./twrWasmAsyncModule": "./lib-js/twrasyncmod.js",
    "./twrCanvas": "./lib-js/twrcanvas.js",
    "./twrCircular": "./lib-js/twrcircular.js",
    "./twrDiv": "./lib-js/twrdiv.js"
  },
  "contributors": [
    {
      "name": "Anthony Wood",
      "email": "ajwood1965@gmail.com"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/twiddlingbits/tiny-wasm-runtime.git"
  },
  "license": "MIT",
  "dependencies": {
    "whatkey": "^2.0.1"
  }
}
