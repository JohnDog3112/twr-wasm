{
  "name": "tiny-wasm-runtime",
  "version": "2.0.1",
  "description": "A lightweight and easy to use wasm runtime to execute C/C++ code in a browser using Web Assembly.",
  "keywords": [
    "wasm",
    "wasm runtime",
    "Web Assembly",
    "Web Assembly Runtime",
    "runtime",
    "crt",
    "light",
    "lightweight",
    "easy",
    "simple",
    "small",
    "fft",
    "win32",
    "C/C++",
    "typescript",
    "emscripten"
  ],
  "files": [
    "./examples/*/*.html",
    "./examples/*/*.c",
    "./examples/*/*.cpp",
    "./examples/*/*.h",
    "./examples/*/*.ts",
    "./examples/*/*.js",
    "./examples/*/*.txt",
    "./examples/*/*.wasm",
    "./examples/*/package.json",
    "./examples/*/tsconfig.json",
    "./examples/*/Makefile*",
    "./examples/*",
    "./include/**",
    "./lib-c/*",
    "./lib-js/*",
    "./LICENSE",
    "./readme*"
  ],
  "module": "./lib-js/index.js",
  "exports": {
    ".": "./lib-js/index.js",
    "./twrWasmModule": "./lib-js/twrmod.js",
    "./twrWasmModuleAsync": "./lib-js/twrmodasync.js"
  },
  "contributors": [
    {
      "name": "Anthony Wood",
      "email": "ajwood1965@gmail.com"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/twiddlingbits/tiny-wasm-runtime.git"
  },
  "license": "MIT",
  "homepage": "https://twiddlingbits.dev/"
}
