{
    "configurations": [
    {
            "name": "(gdb) Launch tiny-wasm-crt unit tests",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/unit-test/out/test.exe",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/unit-test/out/",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "miDebuggerPath": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Set Disassembly Flavor to Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ]
    }
 ]
}